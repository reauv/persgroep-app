{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "no-console": 0,
    "arrow-body-style": 0,
    "indent": [2, "tab"],
    "react/jsx-indent-props": [2, "tab"],
    "no-unused-expressions": 0
  },
  "globals": {
    "it": true,
    "describe": true,
    "before": true,
    "beforeEach": true,
    "after": true,
    "afterEach": true,
    "expect": true
  }
}
