{
  "name": "redux-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for webpack, redux & react",
  "scripts": {
    "start": "rm -rf ./build & nodemon -w server server",
    "production": "rm -rf ./build && webpack -p --config webpack.production.config.js",
    "lint": "eslint src",
    "test": "rm -rf ./coverage && karma start internals/testing/karma.conf.js",
    "test:watch": "npm run test -- --auto-watch --no-single-run --reporters mocha",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "author": "Robert van Steen <studio@rovansteen.nl> (http://github.com/reauv)",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-core": "^6.9.1",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.4.2",
    "classnames": "^2.2.5",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.16.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "fixture-factory": "^1.5.1",
    "html-webpack-plugin": "^2.9.0",
    "imports-loader": "^0.6.5",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.2.1",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.0.4",
    "karma-sinon-chai": "github:adambrunner/karma-sinon-chai",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack-with-fast-source-maps": "^1.9.2",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.3.1",
    "nodemon": "^1.9.2",
    "nyc": "^6.4.4",
    "postcss-loader": "^0.8.1",
    "postcss-modules-values": "^1.1.1",
    "postcss-nested": "^1.0.0",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1",
    "webpack-node-externals": "^1.2.0"
  },
  "dependencies": {
    "axios": "^0.12.0",
    "babel-polyfill": "^6.9.1",
    "cookie-parser": "^1.4.3",
    "dotenv-safe": "^2.3.1",
    "draft-js": "^0.7.0",
    "draft-js-export-html": "^0.3.0",
    "es6-promise": "^3.0.2",
