{
  "name": "redux-webpack-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for webpack, redux & react",
  "scripts": {
    "start": "rm -rf ./build & nodemon -w server.js server.js",
    "production": "rm -rf ./build && webpack -p --config webpack.production.config.js",
    "lint": "eslint src",
    "test": "mocha-webpack --webpack-config webpack.test.config.js \"src/**/*.spec.js\" --colors",
    "test:watch": "mocha-webpack --webpack-config webpack.test.config.js \"src/**/*.spec.js\" --watch --colors"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Robert van Steen <studio@rovansteen.nl> (http://github.com/reauv)",
  "license": "MIT",
