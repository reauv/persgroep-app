    "js-cookie": "^2.1.2",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.0.1",
    "lodash": "^4.13.1",
    "mobx": "~2.2.2",
    "mobx-react": "^3.3.0",
    "mobx-react-devtools": "^4.2.0",
    "momentjs": "^1.1.7",
    "normalizr": "^2.1.0",
    "null-loader": "^0.1.1",
    "postcss-extend": "^1.0.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-motion-slider": "^0.4.1",
    "react-router": "^2.0.0",
    "react-swipe": "^5.0.3",
    "recompose": "^0.19.0",
    "swipe-js-iso": "^2.0.3",
    "validate.js": "^0.10.0",
    "webpack-dotenv-plugin": "^1.2.0"
  }
